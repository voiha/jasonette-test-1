{
    "$jason": {
      "head": {
        "actions": {
          "visit": [
            {
              "{{#if /\\/edit/.test($jason.url)}}": {
                "type": "$href",
                "options": {
                  "url": "{{$jason.url.replace('/edit', '')}}",
                  "preload": {
                    "background": "#ffffff"
                  }
                }
              }
            },
            {
              "{{#else}}": {
                "type": "$default"
              }
            }
          ]
        }
      },
      "body": {
        "background": {
          "type": "html",
          "url": "https://developers.mypos.eu/",
          "action": {
            "trigger": "visit"
          }
        },
        "layers": [{
            "type": "label",
            "text": "ðŸ‘ˆ",
            "style": {
              "top": "10",
              "right": "10",
              "width": "100",
              "height": "100"
            },
            "action": {
              "type": "$back"
            }
          }]
      }
    }
  }